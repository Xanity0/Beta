BoogaBooga.TextSize = 12.000
BoogaBooga.TextWrapped = true
BoogaBooga.MouseButton1Down:Connect(function()
	--Error Screen (Not Found)

	local ScreenGui = Instance.new("ScreenGui")
	local NotFoundFrame = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")
	local TextLabel_2 = Instance.new("TextLabel")
	local ExitButton = Instance.new("TextButton")
	local TextLabel_3 = Instance.new("TextLabel")

	--Properties:

	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

	NotFoundFrame.Name = "Not Found Frame"
	NotFoundFrame.Parent = ScreenGui
	NotFoundFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	NotFoundFrame.Position = UDim2.new(0.324427485, 0, 0.202105984, 0)
	NotFoundFrame.Size = UDim2.new(0, 367, 0, 271)

	TextLabel.Parent = NotFoundFrame
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.Position = UDim2.new(0.228882834, 0, 0.0221402198, 0)
	TextLabel.Size = UDim2.new(0, 200, 0, 50)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = "Error!!"
	TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 14.000
	TextLabel.TextWrapped = true

	TextLabel_2.Parent = NotFoundFrame
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel_2.BackgroundTransparency = 1.000
	TextLabel_2.Position = UDim2.new(0.114441417, 0, 0.206642061, 0)
	TextLabel_2.Size = UDim2.new(0, 279, 0, 108)
	TextLabel_2.Font = Enum.Font.SourceSans
	TextLabel_2.Text = "If you are seeing this do not worry. This is just indicating there is no script for this game at the current moment or I just didnt find one! Ok have a safe one."
	TextLabel_2.TextColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel_2.TextScaled = true
	TextLabel_2.TextSize = 14.000
	TextLabel_2.TextWrapped = true

	ExitButton.Name = "ExitButton"
	ExitButton.Parent = NotFoundFrame
	ExitButton.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	ExitButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
	ExitButton.Position = UDim2.new(0.85013628, 0, 0.0221402217, 0)
	ExitButton.Size = UDim2.new(0, 39, 0, 36)
	ExitButton.Font = Enum.Font.SourceSans
	ExitButton.Text = "X"
	ExitButton.TextColor3 = Color3.fromRGB(255, 0, 0)
	ExitButton.TextSize = 14.000

	TextLabel_3.Parent = NotFoundFrame
	TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel_3.BackgroundTransparency = 1.000
	TextLabel_3.Position = UDim2.new(0.144414172, 0, 0.645756483, 0)
	TextLabel_3.Size = UDim2.new(0, 257, 0, 85)
	TextLabel_3.Font = Enum.Font.SourceSans
	TextLabel_3.Text = "Please message me on the discord server and lmk you are seeing this screen and if you are I will try to find a script to fix this! Thank you."
	TextLabel_3.TextColor3 = Color3.fromRGB(255, 0, 0)
	TextLabel_3.TextScaled = true
	TextLabel_3.TextSize = 14.000
	TextLabel_3.TextWrapped = true

	-- Scripts:

	local function RTAZAIY_fake_script() -- ExitButton.LocalScript 
		local script = Instance.new('LocalScript', ExitButton)

		script.Parent.MouseButton1Click:Connect(function()
			script.Parent.Parent.Visible = false
		end)
	end
	coroutine.wrap(RTAZAIY_fake_script)()

end)

