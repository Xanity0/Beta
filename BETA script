coroutine.wrap(HDNZ_fake_script)()
local function YUGIH_fake_script() -- SearchForAGame.Script 
	local script = Instance.new('Script', SearchForAGame)

	script.Parent.MouseButton1Click:Connect(function()
		wait(0.2)
		script.Parent.Parent.SearchTool.Visible = true
	end)
	local T = script.Parent
	local on=false
	local f=script.Parent.Parent.SearchFrame
	
	function click()
	
		if on==false then
	
			on=true
	
			f:TweenPosition(UDim2.new(1, 0, 0, 0),"Out","Bounce",0.5) --Change to the position your gui will be when its open!
	
			T.Text = "Close Search"
	
		else
	
	
			on=false
	
			f:TweenPosition(UDim2.new(-800, 0, 0.312, 0),"Out") --Change to the position your gui will be when its closed!
	
			T.Text = "Open Search"
	
	
		end
	
	end
	
	
	
	
	script.Parent.MouseButton1Click:connect(click)
end
coroutine.wrap(YUGIH_fake_script)()
local function LMXRJ_fake_script() -- SearchForAGame.Script 
	local script = Instance.new('Script', SearchForAGame)

	script.Parent.MouseButton1Click:Connect(function()
		wait(0.2)
		script.Parent.Parent.SearchFrame.Visible = true
	end)
	script.Parent.Visible = false
	wait(0.3)
	script.Parent.Visible = true
	local T = script.Parent
	local on=false
	local f=script.Parent.Parent.SearchTool
	
	function click()
	
		if on==false then
	
			on=true
	
			f:TweenPosition(UDim2.new(0.999, 0, -0, 0),"Out",0.5) --Change to the position your gui will be when its open!
	
			T.Text = "Close Search"
	
		else
	
			on=false
	
			f:TweenPosition(UDim2.new(-800, 0),"Out") --Change to the position your gui will be when its closed!
	
			T.Text = "Open Search"
	
		end
	
	end
	
	
	
	script.Parent.MouseButton1Click:connect(click)
end
coroutine.wrap(LMXRJ_fake_script)()
