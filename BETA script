Discord.Name = "Discord"
Discord.Parent = Tools
Discord.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Discord.BorderColor3 = Color3.fromRGB(255, 255, 255)
Discord.Position = UDim2.new(0.417110592, 0, 0.449641526, 0)
Discord.Size = UDim2.new(0, 107, 0, 28)
Discord.Font = Enum.Font.GothamBlack
Discord.Text = "Discord"
Discord.TextColor3 = Color3.fromRGB(255, 255, 255)
Discord.TextScaled = true
Discord.TextSize = 16.000
Discord.TextWrapped = true
Discord.MouseButton1Down:connect(function()
	local message = "Discord"

	math.randomseed(tick())
	local ChatMain = require(game:GetService("Players").LocalPlayer.PlayerScripts.ChatScript.ChatMain)

	local function bypass()
		ChatMain.MessagePosted:fire("LookatChat"..math.random(10000,1000000))
		ChatMain.MessagesChanged:fire(math.random(100000,1000000))
	end

	for v in message:gmatch"." do
		wait(.5)
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(v, "All")
		wait(.5)
		bypass()
	end
end)

