BoogaBooga.Name = "Booga Booga"
BoogaBooga.Parent = SpearmintGuiV2
BoogaBooga.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
BoogaBooga.BorderColor3 = Color3.fromRGB(255, 255, 255)
