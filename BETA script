local function XGYE_fake_script() -- Tools.LocalScript 
	local script = Instance.new('LocalScript', Tools)

	local scroll = script.Parent.Tools3
	local Search = script.Parent.Search
	
	Search.Changed:Connect(function()
		local text = Search.Text:lower()
		if text ~= "" then
			local buttons = scroll:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					local buttonText = button.Text :lower()
					if string.find(buttonText, text) then
						button.Visible = true
					else
						button.Visible = false
					end
				end 
			end
		else
			local buttons = scroll:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					button.Visible = true
				end 
			end
		end
	end)
end
