-- Scripts:

local function MAADRZ_fake_script() -- SpearmintGuiV2.LocalScript 
	local script = Instance.new('LocalScript', SpearmintGuiV2)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(K)
		if K == "=" then
			script.Parent.Visible = true
		end
	end)
end
coroutine.wrap(MAADRZ_fake_script)()
local function VYTNZ_fake_script() -- SpearmintGuiV2.LocalScript 
	local script = Instance.new('LocalScript', SpearmintGuiV2)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(K)
		if K == "-" then
			script.Parent.Visible = false
		end
	end)
end
coroutine.wrap(VYTNZ_fake_script)()
