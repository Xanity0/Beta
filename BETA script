coroutine.wrap(IMBQ_fake_script)()
local function BFUG_fake_script() -- SpearmintGuiV2.LocalScript 
	local script = Instance.new('LocalScript', SpearmintGuiV2)

	local scroll = script.Parent.SearchFrame
	local Search = script.Parent.SearchTool
	
	Search.Changed:Connect(function()
		local text = Search.Text:lower()
		if text ~= "" then
			local buttons = scroll:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					local buttonText = button.Text :lower()
					if string.find(buttonText, text) then
						button.Visible = true
					else
						button.Visible = false
					end
				end 
			end
		else
			local buttons = scroll:GetDescendants()
			for _, button in pairs(buttons) do
				if button:IsA("TextButton") then
					button.Visible = true
				end 
			end
		end
	end)
end
coroutine.wrap(BFUG_fake_script)()
